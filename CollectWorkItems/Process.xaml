                    <InArgument x:TypeArguments="sd:DataRow">[row]</InArgument>
                  </InvokeMethod>
                </Sequence>
              </If.Then>
            </If>
          </Sequence>
        </ActivityAction>
      </ui:ForEachRow.Body>
    </ui:ForEachRow>
    <If Condition="[ValidWorkItems.Rows.Count &gt; 0]" DisplayName="If valid Work Item found" sap2010:WorkflowViewState.IdRef="If_2">
