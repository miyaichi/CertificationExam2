            <DelegateInArgument x:TypeArguments="sd:DataRow" Name="row" />
          </ActivityAction.Argument>
          <Sequence DisplayName="Body" sap2010:WorkflowViewState.IdRef="Sequence_4">
            <If Condition="[row(&quot;Type&quot;).Equals(&quot;WI5&quot;) AND row(&quot;Status&quot;).Equals(&quot;Open&quot;) AND CINT(row(&quot;WIID&quot;)) &lt; 500000]" DisplayName="If WorkItem is valid, add to ValidWorkItems" sap2010:WorkflowViewState.IdRef="If_1">
              <If.Then>
